{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"typeRoots": ["./src/types"],
		"noImplicitReturns": false,
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"src/*": ["src/*"]
		},
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": false,
		"resolveJsonModule": true,
		"noImplicitAny": true
	},
	"include": [
		"**/*.ts",
		"metro.config.js",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"node_modules",
		"babel.config.js",
		"index.js"
	]
}
